/* pages/bagClaim/index.wxss */
.bagClaimWrap {
  width: 100%;
}

.bagClaim {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-top: 280rpx;
}

.imgWrap {
  width: 310rpx;
  height: 310rpx;
  position: relative;
}

.ellipse {
  width: 100%;
  height: 100%;
}

.doorWrap {}

.door {
  width: 90rpx;
  height: 130rpx;
  position: absolute;
  top: 64rpx;
  left: 0;
  right: 0;
  margin: auto;
}

.img_text {
  width: 200rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  position: absolute;
  left: 0;
  right: 0;
  bottom: 58rpx;
  margin: auto;
  font-size: 46rpx;
  font-family: Hiragino Sans GB;
  font-weight: bold;
  color: #2043FF;
}

.bagClaimText {
  margin-top: 76rpx;
  font-size: 46rpx;
  font-family: Hiragino Sans GB;
  font-weight: bold;
  color: #333333;
}

.btn {
  width: 282rpx;
  height: 94rpx;
  text-align: center;
  line-height: 94rpx;
  background: #2043FF;
  border-radius: 18rpx;
  color: #fff;
  font-size: 46rpx;
  font-family: Hiragino Sans GB;
  font-weight: bold;
  margin-top: 144rpx;
}