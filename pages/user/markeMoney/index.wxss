/* pages/user/markeMoney/index.wxss */
.userWrap {
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  color: #333;
}
.marketMoneyWrap {
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.marketMoney {
  width: 100%;
  height: 200rpx;
  background-color: #fff;
  /* border-radius: 10rpx; */
  display: flex;
  justify-content: space-between;
  /* box-shadow: 5px 5px 2px #ddd; */
}

.addMoney {
  flex: 1;
  width: 49%;
  color: #333;
  margin: 30rpx 0;
  text-align: center;
  font-size: 36rpx;
  font-weight: bold;
  padding: 10rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.addMoney:first-child {
  border-right: 1px solid #dedede;
}

.sub_account{
  width: 97%;
  margin-top: 30rpx;
  background-color: #fff;
  border-radius: 8rpx;
  padding: 10rpx;
}
.tabTit{
  font-weight: bold;
}
.publicStyle{
  width: 100%;
  display: flex;
  flex-wrap: nowrap;
  justify-content: space-between;
  line-height: 80rpx;
  border-bottom: 1rpx solid #dedede;
  font-size: 28rpx;
}
.sub_accountTitle{
  display: flex;
  justify-content: left;
  padding: 10rpx 0;
}
.box{
  width: 6rpx;
  height: 40rpx;
  background-color: #0042BB;
  margin-right: 16rpx;
}
.title{
  font-size: 32rpx;
  font-weight: bold;
}
.flex1{
 flex: 1;
 text-align: center;
}
.colorStyle{
  color: #3399FF;
}
/* icon */
.iconWrap{
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  padding-top: 300rpx;
}
.iconNone{
  display: none;
}
.icon{
  font-size: 400rpx;
  color: #3C77E4;
}
.icon_text{
  color: #8a9219;
}

/* 进度条 */
.cu-progress{
  height: 16rpx;
}